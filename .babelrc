{
    "presets": ["@babel/preset-env", "@babel/preset-typescript"],
    "plugins": [
        "@babel/plugin-transform-runtime",
        [
            "module-resolver",
            {
                "root": ["./src"],
                "alias": {
                    "@services": "./src/services",
                    "@controllers": "./src/controllers",
                    "@models": "./src/models",
                    "@data-access": "./src/data-access",
                    "@helpers": "./src/helpers"
                }
            }
        ]
    ]
}
